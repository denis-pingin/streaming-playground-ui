{
  "name": "streaming-playground-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.10",
    "@material-ui/icons": "^4.9.1",
    "@opentok/client": "^2.17.0",
    "@sentry/browser": "^5.15.4",
    "aws-amplify": "^3.0.7",
    "notistack": "^0.9.9",
    "react": "^16.13.1",
    "react-context-hook": "^2.1.0",
    "react-dom": "^16.13.1",
    "react-icons": "^3.9.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-stripe-elements": "^6.1.1",
    "react-use-websocket": "^1.3.4",
    "typeface-roboto": "0.0.75"
  },
  "scripts": {
    "start": "REACT_APP_STAGE=offline BROWSER=false react-scripts start --no-cache --inspect-brk --verbose=false --runInBand",
    "start-https": "REACT_APP_STAGE=offline BROWSER=false HTTPS=true react-scripts start --no-cache --inspect-brk --verbose=false --runInBand",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "deploy": "./sync-s3.sh",
    "all": "npm run build && npm run deploy",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-plugin-react-hooks": "^3.0.0"
  }
}
